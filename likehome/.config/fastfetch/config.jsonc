// Load with --load-config examples/2.jsonc
// Note that you must replace the image path to an existing image to display it.

{
	"$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
	"logo": {
		"source": "linux",
		"padding": {
			"top": 8,
			"left": 3
		},
		"width": 50,
		"height": 28,
		"color": {
			"1": "\u001b[39",
			"2": "\u001b[90"
		}
	},

	"display": {
		"separator": " -> "
	},
	"modules": [
		{
			"type": "custom",
			"format": "\u001b[90m┌────────────────────────────────────────────────────────────┐"
		},
		{
			"type": "title",
			"keyWidth": 10
		},
		{
			"type": "custom",
			"format": "\u001b[90m└────────────────────────────────────────────────────────────┘"
		},
		{
			"type": "custom",
			"format": " \u001b[33m󰮯   \u001b[37m󰊠   \u001b[31m󰊠   \u001b[32m󰊠   \u001b[33m󰊠   \u001b[34m󰊠   \u001b[35m󰊠   \u001b[36m󰊠   \u001b[39m󰊠 "
		},
		{
			"type": "custom",
			"format": "\u001b[90m┌────────────────────────────────────────────────────────────┐"
		},
		{
			"type": "os",
			"key": " OS",
			"keyColor": "yellow"
		},
		{
			"type": "kernel",
			"format": "{1} {2}",
			"key": "│ ├ Kernel",
			"keyColor": "yellow"
		},
		{
			"type": "packages",
			"key": "│ ├󰏖 Packages",
