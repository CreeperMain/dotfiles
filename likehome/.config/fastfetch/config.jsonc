			"keyColor": "blue"
		},
		{
			"type": "theme",
			"key": "│ ├󰉼 Theme",
			"keyColor": "blue"
		},
		{
			"type": "wmtheme",
			"key": "│ ├󰉼 Theme",
			"keyColor": "blue"
